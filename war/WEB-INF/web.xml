<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>ArticulateAppGetServlet</servlet-name>
		<servlet-class>edu.sjsu.articulate.ArticulateAppGetServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ArticulateAppGetServlet</servlet-name>
		<url-pattern>/articulateAppGetData</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ArticulateAppSetServlet</servlet-name>
		<servlet-class>edu.sjsu.articulate.ArticulateAppSetServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ArticulateAppSetServlet</servlet-name>
		<url-pattern>/articulateAppSetData</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>GetCategoriesServlet</servlet-name>
		<servlet-class>edu.sjsu.articulate.category.GetCategoriesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetCategoriesServlet</servlet-name>
		<url-pattern>/getCategories</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SetCategoriesServlet</servlet-name>
		<servlet-class>edu.sjsu.articulate.category.SetCategoriesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SetCategoriesServlet</servlet-name>
		<url-pattern>/setCategories</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
